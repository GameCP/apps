{
  "name": "brand-kit",
  "version": "1.0.12",
  "description": "Add custom branding to dashboards",
  "main": "dist/index.js",
  "scripts": {
    "preinstall": "node ../scripts/resolve-file-deps.js || true",
    "build": "gamecp-build",
    "version:major": "gamecp-build version major",
    "version:minor": "gamecp-build version minor",
    "version:patch": "gamecp-build version patch",
    "release": "gamecp-build release",
    "validate": "tsc --noEmit",
    "deploy": "../deploy.sh $(basename $(pwd)) --env local --target local",
    "deploy:prod": "../deploy.sh $(basename $(pwd)) --env prod --target appstore",
    "deploy:all": "../deploy.sh $(basename $(pwd)) --env prod --target all"
  },
  "devDependencies": {
    "@gamecp/build": "file:../../packages/build",
    "@gamecp/types": "file:../../packages/types",
    "@gamecp/ui": "file:../../packages/ui",
    "@types/node": "^20.10.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "typescript": "^5.3.3"
  },
  "author": "GameCP Team",
  "license": "MIT",
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  }
}
