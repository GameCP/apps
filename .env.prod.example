# GameCP Extension Deploy - Production
# Copy to .env.prod and fill in values

# MongoDB connection for production (all tenant DBs)
MONGODB_URI=mongodb+srv://user:password@cluster.mongodb.net

# App Store API (production)
APPSTORE_API_URL=https://gamecp.com

# Your publisher API key (for app store publishing)
GAMECP_PUBLISHER_KEY=your-production-key-here
